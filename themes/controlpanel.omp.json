{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{ if .Root }} \u26A1 {{ end }}{{ .Folder | replace \"~\" \"\ud83c\udfe0\" }} @ {{ .HostName }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "template": "{{ if .WSL }} \ue712 on {{ end }}<#2ca9e6>{{ .Icon }} </>",
          "style": "diamond",
          "leading_diamond": "\u2593</>",
          "trailing_diamond": "<#2f2f2f,transparent>\ue0b0</>",
          "foreground": "#eeeeee",
          "background": "#2f2f2f",
          "min_width": 94,
          "properties": {
            "arch": "\uf303",
            "debian": "\uf306",
            "fedora": "\uf30a",
            "linux": "\ue712",
            "macos": "\ue711",
            "mint": "\uf30f",
            "ubuntu": "\uf31c",
            "windows": "\ue70f"
          }
        },
        {
          "type": "shell",
          "template": "<#979797>\uf489 </>{{ .Name }}",
          "style": "diamond",
          "leading_diamond": "<transparent,#2f2f2f>\ue0b0</>",
          "trailing_diamond": "<#2f2f2f,transparent>\ue0b0</>",
          "foreground": "#eeeeee",
          "background": "#2f2f2f",
          "min_width": 84
        },
        {
          "type": "session",
          "template": "<#979797>\uf26c </>{{ if .SSHSession }}<#1a1a1a>\ueba9 </>{{ end }}{{ .HostName }}",
          "style": "diamond",
          "leading_diamond": "<transparent,#2f2f2f>\ue0b0</>",
          "trailing_diamond": "<#2f2f2f,transparent>\ue0b0</>",
          "foreground": "#eeeeee",
          "background": "#2f2f2f"
        },
        {
          "type": "text",
          "template": "<#40E0D0>\uf2c0 </>{{ .UserName }}",
          "style": "diamond",
          "leading_diamond": "<transparent,#2f2f2f>\ue0b0</>",
          "trailing_diamond": "<#2f2f2f,transparent>\ue0b0</>",
          "foreground": "#eeeeee",
          "background": "#2f2f2f"
        },
        {
          "type": "root",
          "template": " \uf0e7 ",
          "style": "diamond",
          "leading_diamond": "<transparent,#2f2f2f>\ue0b0</>",
          "trailing_diamond": "<#2f2f2f,transparent>\ue0b0</>",
          "foreground": "#ffff66",
          "background": "#2f2f2f"
        },
        {
          "type": "python",
          "template": "<#32CD32>Óàµ </>{{ if .Venv }}[{{ .Venv }}]{{ end }}{{ .Full }}",
          "style": "diamond",
          "leading_diamond": "<transparent,#2f2f2f>\ue0b0</>",
          "trailing_diamond": "<#2f2f2f,transparent>\ue0b0</>",
          "foreground": "#eeeeee",
          "background": "#2f2f2f",
          "properties": {
            "display_mode": "context",
            "fetch_version": true,
            "fetch_virtual_env": true
          }
        },
        {
          "type": "node",
          "template": "<#68A063>Ó¥ç </>{{ .Full }}",
          "style": "diamond",
          "leading_diamond": "<transparent,#2f2f2f>\ue0b0</>",
          "trailing_diamond": "<#2f2f2f,transparent>\ue0b0</>",
          "foreground": "#eeeeee",
          "background": "#2f2f2f"
        },
        {
          "type": "npm",
          "template": "<#cb3837>Ó¥é </>",
          "style": "diamond",
          "leading_diamond": "<transparent,#2f2f2f>\ue0b0</>",
          "trailing_diamond": "<#2f2f2f,transparent>\ue0b0</>",
          "foreground": "#eeeeee",
          "background": "#2f2f2f"
        },
        {
          "type": "gcp",
          "template": "<#ff4500>Û±á∂ </>{{.Project}}",
          "style": "diamond",
          "foreground": "#eeeeee",
          "background": "#2f2f2f",
          "leading_diamond": "<transparent,#2f2f2f>\ue0b0</>",
          "trailing_diamond": "<#2f2f2f,transparent>\ue0b0</>"
        },
        {
          "type": "git",
          "template": "<#F14E32>Ôáì  </>{{ .UpstreamIcon }}{{ .HEAD }}{{ if .BranchStatus }} \ue727 {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \ue729 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \ue728 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#e4cf6a{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f78c6c{{ end }}",
            "{{ if gt .Ahead 0 }}#C792EA{{ end }}",
            "{{ if gt .Behind 0 }}#c792ea{{ end }}"
          ],
          "style": "diamond",
          "leading_diamond": "<transparent,#2f2f2f>\ue0b0</>",
          "trailing_diamond": "<#2f2f2f,transparent>\ue0b0</>",
          "foreground": "#eeeeee",
          "background": "#2f2f2f",
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_stash_count": true,
            "fetch_worktree_count": true,
            "branch_icon": "\ue725",
            "mapped_branches": {
              "feat/*": "üöÄ ",
              "bug/*": "üêõ "
            },
            "source": "cli"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "filler": " ",
      "segments": [
        {
          "type": "sysinfo",
          "template": "{{ round .PhysicalPercentUsed .Precision }}%<#979797>\ue266 </>",
          "style": "diamond",
          "leading_diamond": "<#2f2f2f,transparent>\ue0b2</>",
          "trailing_diamond": "<transparent,#2f2f2f>\ue0b2</>",
          "foreground": "#eeeeee",
          "background": "#2f2f2f",
          "min_width": 104
        },
        {
          "type": "time",
          "style": "diamond",
          "leading_diamond": "<#2f2f2f,transparent>\ue0b2</>",
          "trailing_diamond": "<transparent,#2f2f2f>\ue0b2</>",
          "template": "{{ .CurrentDate | date \"2006-01-02\" }}<#bda57f>\uf073 </>",
          "foreground": "#eeeeee",
          "background": "#2f2f2f",
          "min_width": 74,
          "properties": {
            "time_format": "2006/01/02"
          }
        },
        {
          "type": "time",
          "style": "diamond",
          "leading_diamond": "<#2f2f2f,transparent>\ue0b2</>",
          "trailing_diamond": "\u2593</>",
          "template": "{{ .CurrentDate | date \"15:04:05\" }}<#bda57f>\ue641 </>",
          "foreground": "#eeeeee",
          "background": "#2f2f2f",
          "properties": {
            "time_format": "15:04:05"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "overflow": "hide",
      "segments": [
        {
          "type": "path",
          "template": "<><#FFD700>\ue5ff </><#7a7a7a>\ue0b1 </>{{ .Path }}",
          "style": "diamond",
          "foreground": "#eeeeee",
          "background": "#2f2f2f",
          "leading_diamond": "\u2593</>",
          "trailing_diamond": "<#2f2f2f>\ue0b0</>",
          "properties": {
            "home_icon": "<#D2B48C>\uf015 </>",
            "folder_separator_icon": "\uf061 ",
            "style": "full"
          }
        },
        {
          "type": "text",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "\ue285\ue285\ue285 ",
          "foreground": "#00FF00",
          "background": "transparent"
        }
      ]
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "filler": " ",
      "overflow": "hide",
      "segments": [
        {
          "type": "status",
          "template": "{{ if eq .Segments.Executiontime.FormattedMs \"0ms\" }}\uf0a9{{ else if gt .Code 0 }}<#FF0000>[{{ .Code }}]X</>{{ else }}<#00FF00>[0]\uf00c</>{{ end }} ",
          "style": "diamond",
          "leading_diamond": "<#2f2f2f,transparent>\ue0b2</>",
          "trailing_diamond": "<transparent,#2f2f2f>\ue0b2</>",
          "foreground": "#eeeeee",
          "background": "#2f2f2f",
          "properties": {
            "always_enabled": true
          }
        },
        {
          "type": "executiontime",
          "template": "{{ .FormattedMs }}<#cccccc>\uf252 </>",
          "style": "diamond",
          "leading_diamond": "<#2f2f2f,transparent>\ue0b2</>",
          "trailing_diamond": "\u2593</>",
          "foreground": "#eeeeee",
          "background": "#2f2f2f",
          "properties": {
            "threshold": 0
          }
        }
      ]
    }
  ],
  "transient_prompt": {
    "template": "<#2f2f2f>\u2593</><#eeeeee,#2f2f2f><#FFD700>\ue5ff </><#7a7a7a>\ue0b1 </><#eeeeee>{{ .Segments.Path.Path }}</><#2f2f2f,transparent>\ue0b0</><#eeeeee,transparent>\ue285\ue285\ue285 </>"
  },
  "version": 3
}
